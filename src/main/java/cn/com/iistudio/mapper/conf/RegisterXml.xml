<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.iistudio.mapper.RegisterMapper">
 
<insert id="writeTourist" parameterType="cn.com.iistudio.entity.User">
  
  INSERT  INTO TouristUser(NickName,UserName,Password,Gender,HeadImage,Email,Tel) VALUES(#{nickName},#{userName},#{password},#{gender},#{headImage},#{email},#{tel})
  
  </insert>
  
</mapper>